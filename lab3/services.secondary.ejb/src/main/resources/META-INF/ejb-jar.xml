<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd"
         version="3.2">
    <enterprise-beans>
        <session>
            <ejb-name>MoviesServiceBean</ejb-name>
            <env-entry>
                <env-entry-name>main-service-url</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>https://localhost:8080</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>movies-endpoint</env-entry-name>
                <env-entry-type>java.lang.String</env-entry-type>
                <env-entry-value>/api/movies</env-entry-value>
            </env-entry>
            <env-entry>
                <env-entry-name>losers-oscars-count</env-entry-name>
                <env-entry-type>java.lang.Integer</env-entry-type>
                <env-entry-value>1</env-entry-value>
            </env-entry>
        </session>
    </enterprise-beans>
</ejb-jar>